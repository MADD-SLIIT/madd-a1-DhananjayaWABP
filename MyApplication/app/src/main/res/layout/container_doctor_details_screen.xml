  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/container_doctor_details_screen"
  android:layout_width="match_parent"
  android:layout_height="812dp"
  android:background="@drawable/container_doctor_details_screen"
  android:gravity="left|top"
  android:orientation="vertical">

  <FrameLayout
    android:id="@+id/container_headline"
    android:layout_width="335dp"
    android:layout_height="30dp"
    android:layout_marginRight="60dp"
    android:layout_marginTop="20dp"
    android:layout_gravity="right">

    <ImageView
      android:id="@+id/image_headline_DoctorDitailback"
      android:layout_width="30dp"
      android:layout_height="30dp"
      android:layout_gravity="left"
      android:scaleType="centerInside"
      android:src="@drawable/image_headline_group2" />
    
    <TextView
      android:id="@+id/text_headline"
      android:layout_width="wrap_content"
      android:layout_height="23dp"
      android:layout_marginLeft="49dp"
      android:layout_marginTop="4dp"
      android:layout_marginBottom="3dp"
      android:layout_gravity="left"
      android:gravity="left|center_vertical"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Doctor Details"
      android:textColor="#FF333333"
      android:textSize="18dp" />
  </FrameLayout>
  
