<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/container_profile_screen"
  android:layout_width="match_parent"
  android:layout_height="812dp"
  android:background="@drawable/container_profile_screen">
  
  <FrameLayout
    android:id="@+id/container_button"
    android:layout_width="295dp"
    android:layout_height="54dp"
    android:layout_marginTop="708dp"
    android:layout_gravity="top|center_horizontal"
    android:background="@drawable/container_button">
    
    <ImageView
      android:id="@+id/image_button_rectangle"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:background="@drawable/image_button_rectangle"
      android:scaleType="centerCrop" />

    <TextView
        android:id="@+id/text_button_save"
        android:layout_width="54dp"
        android:layout_height="23dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="15.5dp"
        android:layout_marginBottom="15.5dp"
        android:gravity="center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Save"
        android:textColor="#FFFFFFFF"
        android:textSize="18dp" />
  </FrameLayout>
  
  <LinearLayout
    android:id="@+id/container_personal_information"
    android:layout_width="match_parent"
    android:layout_height="305dp"
    android:layout_marginLeft="20dp"
    android:layout_marginRight="20dp"
    android:layout_marginTop="-1dp"
    android:layout_marginBottom="136dp"
    android:layout_gravity="bottom"
    android:clipToPadding="false"
    android:gravity="left|top"
    android:orientation="vertical"
    android:paddingTop="1dp">
    
    <TextView
      android:id="@+id/text_personal_information"
      android:layout_width="wrap_content"
      android:layout_height="23dp"
      android:layout_marginTop="-1dp"
      android:layout_marginBottom="-1dp"
      android:layout_gravity="left"
      android:gravity="left|center_vertical"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Personal information"
      android:textColor="#FF333333"
      android:textSize="18dp" />
    
    <FrameLayout
      android:id="@+id/container_name"
      android:layout_width="match_parent"
      android:layout_height="60dp"
      android:layout_marginTop="12dp"
      android:background="@drawable/container_name">
      
      <ImageView
        android:id="@+id/image_name_rectangle"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/image_name_rectangle"
        android:scaleType="centerCrop" />

      <TextView
          android:id="@+id/text_name_title"
          android:layout_width="wrap_content"
          android:layout_height="14dp"
          android:layout_gravity="top|left"
          android:layout_marginLeft="20dp"
          android:layout_marginTop="8dp"
          android:layout_marginBottom="-1dp"
          android:gravity="left|center_vertical"
          android:includeFontPadding="false"
          android:maxLines="1"
          android:singleLine="true"
          android:text="Name"
          android:textColor="#FFF27400"
          android:textSize="20px" />
      
      <TextView
        android:id="@+id/text_name_value"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="27dp"
        android:layout_marginBottom="12dp"
        android:layout_gravity="left"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Abdullah Mamun"
        android:textColor="#FF677294"
        android:textSize="16dp" />
    </FrameLayout>
    
    <FrameLayout
      android:id="@+id/container_contact_number"
      android:layout_width="match_parent"
      android:layout_height="60dp"
      android:layout_marginTop="10dp">
      
      <ImageView
        android:id="@+id/image_contact_rectangle"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/image_contact_rectangle"
        android:scaleType="centerCrop" />

      <TextView
          android:id="@+id/text_contact_number_title"
          android:layout_width="wrap_content"
          android:layout_height="14dp"
          android:layout_gravity="top|left"
          android:layout_marginLeft="20dp"
          android:layout_marginTop="8dp"
          android:layout_marginBottom="-1dp"
          android:gravity="left|center_vertical"
          android:includeFontPadding="false"
          android:maxLines="1"
          android:singleLine="true"
          android:text="Contact Number"
          android:textColor="#FFF27400"
          android:textSize="20px" />
      
      <TextView
        android:id="@+id/text_contact_number_value"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="26.5dp"
        android:layout_marginBottom="12.5dp"
        android:layout_gravity="left"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="+8801800000000"
        android:textColor="#FF677294"
        android:textSize="16dp" />
      
      <ImageView
        android:id="@+id/image_contact_group"
        android:layout_width="15dp"
        android:layout_height="14.93dp"
        android:layout_marginLeft="300dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="23dp"
        android:layout_marginBottom="22.07dp"
        android:scaleType="centerInside"
        android:src="@drawable/image_contact_group" />
    </FrameLayout>
    
    <FrameLayout
      android:id="@+id/container_birth"
      android:layout_width="match_parent"
      android:layout_height="60dp"
      android:layout_marginTop="10dp">
      
      <ImageView
        android:id="@+id/image_birth_rectangle"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/image_birth_rectangle"
        android:scaleType="centerCrop" />

      <TextView
          android:id="@+id/text_birth_date"
          android:layout_width="wrap_content"
          android:layout_height="14dp"
          android:layout_gravity="top|left"
          android:layout_marginLeft="20dp"
          android:layout_marginTop="8dp"
          android:layout_marginBottom="-1dp"
          android:gravity="left|center_vertical"
          android:includeFontPadding="false"
          android:maxLines="1"
          android:singleLine="true"
          android:text="Date of birth"
          android:textColor="#FFF27400"
          android:textSize="20px" />
      
      <TextView
        android:id="@+id/text_birth_dd_mm_yyyy"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="27dp"
        android:layout_marginBottom="12dp"
        android:layout_gravity="left"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="DD MM YYYY"
        android:textColor="#FF677294"
        android:textSize="16dp" />
      
      <ImageView
        android:id="@+id/image_birth_group"
        android:layout_width="15dp"
        android:layout_height="14.93dp"
        android:layout_marginLeft="300dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="23dp"
        android:layout_marginBottom="22.07dp"
        android:scaleType="centerInside"
        android:src="@drawable/image_birth_group" />
    </FrameLayout>
    
    <FrameLayout
      android:id="@+id/container_location"
      android:layout_width="match_parent"
      android:layout_height="0dp"
      android:layout_marginTop="11dp"
      android:layout_weight="1"
      android:background="@drawable/container_location">
      
      <ImageView
        android:id="@+id/image_location_rectangle"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/image_location_rectangle"
        android:scaleType="centerCrop" />

      <TextView
          android:id="@+id/text_location_title"
          android:layout_width="wrap_content"
          android:layout_height="14dp"
          android:layout_gravity="top|left"
          android:layout_marginLeft="20dp"
          android:layout_marginTop="7dp"
          android:layout_marginBottom="-1dp"
          android:gravity="left|center_vertical"
          android:includeFontPadding="false"
          android:maxLines="1"
          android:singleLine="true"
          android:text="Location"
          android:textColor="#FFF27400"
          android:textSize="20px" />
      
      <TextView
        android:id="@+id/text_location_add_details"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="26dp"
        android:layout_marginBottom="13dp"
        android:layout_gravity="left"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Add Details"
        android:textColor="#FF677294"
        android:textSize="16dp" />
    </FrameLayout>
  </LinearLayout>
  
  <FrameLayout
    android:id="@+id/container_profile"
    android:layout_width="match_parent"
    android:layout_height="357dp"
    android:layout_gravity="top">
    
    <ImageView
      android:id="@+id/image_profile_rectangle"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:background="@drawable/image_profile_rectangle"
      android:scaleType="centerCrop" />
    
    <FrameLayout
      android:id="@+id/container_profile_group1"
      android:layout_width="match_parent"
      android:layout_height="75dp"
      android:layout_marginLeft="40dp"
      android:layout_marginRight="39dp"
      android:layout_marginTop="100dp"
      android:layout_gravity="top|center_horizontal"
      android:layout_centerHorizontal="true"
      >

      <TextView
          android:id="@+id/text_profile_setup"
          android:layout_width="147dp"
          android:layout_height="21dp"
          android:layout_gravity="top"
          android:layout_marginLeft="90dp"
          android:layout_marginTop="-1dp"
          android:layout_marginRight="70dp"
          android:layout_marginBottom="-1dp"
          android:gravity="left|center_vertical"
          android:includeFontPadding="false"
          android:maxLines="1"
          android:singleLine="true"
          android:text="Set up your profile"
          android:textColor="#FFFFFFFF"
          android:textSize="16dp" />

      <TextView
          android:id="@+id/text_profile_update"
          android:layout_width="match_parent"
          android:layout_height="46dp"
          android:layout_marginTop="29dp"
          android:gravity="center_horizontal|top"
          android:includeFontPadding="false"
          android:lineHeight="23.46dp"
          android:text="Update your profile to connect your doctor with\nbetter impression."
          android:textColor="#FFFFFFFF"
          android:textSize="30px" />
    </FrameLayout>
    
    <FrameLayout
      android:id="@+id/container_profile_group2"
      android:layout_width="138dp"
      android:layout_height="130dp"
      android:layout_gravity="bottom|center_horizontal"
      android:layout_centerHorizontal="true"
      android:layout_marginBottom="30dp"
     >
      
      <ImageView
        android:id="@+id/container_profile_group3"
        android:layout_width="130dp"
        android:layout_height="match_parent"
        android:layout_gravity="left"
        android:scaleType="centerCrop"
        android:src="@drawable/container_profile_group3" />
      
      <ImageView
        android:id="@+id/image_profile_group"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginBottom="13dp"
        android:layout_gravity="bottom|right"
        android:scaleType="centerInside"
        android:src="@drawable/image_profile_group" />
    </FrameLayout>
  </FrameLayout>
  
  <FrameLayout
    android:id="@+id/container_headline"
    android:layout_width="107dp"
    android:layout_height="30dp"
    android:layout_marginLeft="20dp"
    android:layout_marginTop="36dp"
    android:layout_gravity="top|left">
    
    <ImageView
      android:id="@+id/image_headline_group"
      android:layout_width="30dp"
      android:layout_height="30dp"
      android:layout_gravity="left"
      android:scaleType="centerInside"
      android:src="@drawable/image_headline_group" />
    
    <TextView
      android:id="@+id/text_headline_profile"
      android:layout_width="match_parent"
      android:layout_height="23dp"
      android:layout_marginLeft="49dp"
      android:layout_marginTop="4dp"
      android:layout_marginBottom="3dp"
      android:gravity="left|center_vertical"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Profile"
      android:textColor="#FFFFFFFF"
      android:textSize="18dp" />
  </FrameLayout>
</FrameLayout>
